{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "single"],
    "eslint-env": 0,
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "func-names": "off",
    "no-tabs": "off",
    "no-undef": "off"
  }
}
