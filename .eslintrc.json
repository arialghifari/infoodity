{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "single"],
    "eslint-env": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "object-curly-newline": 0,
    "operator-linebreak": 0,
    "func-names": 0,
    "no-tabs": 0,
    "no-undef": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "import/prefer-default-export": 0,
    "no-return-assign": 0
  }
}
