{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "single"],
      "eslint-env": "es6"
    }
  }
  